VAR2:=TDXDLL8(1,PERIOD,H,L);{得到笔}
VAR3:=TDXDLL8(2,0,0,0);{得到段}

BAR1:=BARSLAST(VAR3=-1);
V1:=REF(L,BAR1);
BAR2:=SUMBARS(VAR3=-1,2)-1;
V2:=REF(L,BAR2);
BAR3:=SUMBARS(VAR3=-1,3)-1;
V3:=REF(L,BAR3);
PAR1:=BARSLAST(VAR3=1);
G1:=REF(H,PAR1);
PAR2:=SUMBARS(VAR3=1,2)-1;
G2:=REF(H,PAR2);
PAR3:=SUMBARS(VAR3=1,3)-1;
G3:=REF(H,PAR3);